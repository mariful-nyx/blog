{
  "version": 2,
  "builds": [
    {
      "src": "bpm/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    },
      {
        "src": "setup.sh",
        "use":"@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },

    {
      "src": "/(.*)",
      "dest": "bpm/wsgi.py"
    }
  ]
}
