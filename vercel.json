{
  "version": 2,
  "builds": [
    {
      "src": "bpm/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 10,
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "buildCommand": "bash setup.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },

    {
      "src": "/(.*)",
      "dest": "bpm/wsgi.py"
    }
  ]
}
